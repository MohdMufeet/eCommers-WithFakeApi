import { NextResponse} from 'next/server'
const get = async() =>{
      const products = [
    { id: 1, title: "Laptop", price: 50000 , image: 'abc.com'},
    { id: 2, title: "Mobile", price: 20000 , image: 'abc.com'},
    { id: 3, title: "Headphones", price: 2000 , image: 'abc.com'},
  ];
  return NextResponse.json(products);
}

export default get;
